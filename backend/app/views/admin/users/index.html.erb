<h1>ユーザー管理</h1>

<table class="table table-admin">
  <thead>
    <tr>
      <th>名称</th>
      <th>権限</th>
      <th>登録日</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.privilege %></td>
    <td><%= user.created_at %></td>
    <td>編集</td>
  </tr>
  <% end %>
</table>

<%= link_to '戻る', root_path %>
